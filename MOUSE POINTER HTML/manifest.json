{
  "manifest_version": 3,
  "name": "Real Illusion Extension",
  "version": "1.0",
  "description": "Displays a random message when you click your mouse.",
"icons": [
    {"16": "path/to/icon16.png"},
    {"32": "path/to/icon32.png"},
    {"48": "path/to/icon48.png"},
    {"128": "path/to/icon128.png"}
],
  "permissions": ["activeTab"],
  "action": {
    "default_icon": "icon16.png",
    "default_title": "Real Illusion Extension",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup.js"]
    }
  ]
}